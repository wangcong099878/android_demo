<?xml version="1.0" encoding="utf-8"?>
<!-- 标签的背景 label_bg -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 标签选中时的背景 -->
    <item android:state_selected="true">
        <shape>
            <solid android:color="?attr/app_skin_primary_color" />
            <corners android:radius="@dimen/category_btn_radio" />
        </shape>
    </item>
    <!-- 标签的正常背景 -->
    <item>
        <shape>
            <corners android:radius="@dimen/category_btn_radio" />
            <solid android:color="@color/qmui_config_color_background" />
        </shape>
    </item>
</selector>