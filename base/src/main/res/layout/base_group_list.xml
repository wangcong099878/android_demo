<?xml version="1.0" encoding="utf-8"?>
<com.qmuiteam.qmui.layout.QMUIFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:qmui_skin_background="?attr/app_skin_background_color_2"
    android:background="?attr/app_skin_background_color_2">

    <the.one.base.widge.overscroll.OverScrollScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:ignore="MissingPrefix">

            <com.qmuiteam.qmui.layout.QMUIFrameLayout
                android:id="@+id/fl_child_top_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.qmuiteam.qmui.widget.grouplist.QMUIGroupListView
                android:id="@+id/groupListView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:qmui_skin_background="?attr/app_skin_background_color_2"
                android:background="?attr/app_skin_background_color_2"/>

            <com.qmuiteam.qmui.layout.QMUIFrameLayout
                android:id="@+id/fl_child_bottom_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>

    </the.one.base.widge.overscroll.OverScrollScrollView>
</com.qmuiteam.qmui.layout.QMUIFrameLayout>